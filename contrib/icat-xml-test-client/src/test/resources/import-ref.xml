<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Does an import while referencing already existing data -->
<icatdata xmlns:ns2="http://icatproject.org">
	<!-- config for client -->
	<config>
		<haltOnError>false</haltOnError>
		<localIdRange>
			<min></min>
			<max>0</max>
		</localIdRange>
		<!-- search data -->
		<searchids>
			<search>
				<id>-1</id>
				<query>Facility [name = 'ESRF']</query>
			</search>
		</searchids>
	</config>
	<!-- load data -->
	<instrument>
		<id>-1</id>
		<name>ID02</name>
		<fullName>Beamline ID02</fullName>
		<description>Second beamline</description>
		<facility>
			<id>-1</id>
		</facility>
		<type>Beamline</type>
	</instrument>
</icatdata>