# URL for oracle (for a local system on XE it is just: url="jdbc:oracle:thin:@//localhost:1521/XE")
url="jdbc:oracle:thin:@//localhost:1521/XE"

icatName=ICAT
icatPW=myicatpasswd

# ICATUSER user name and password in database
icatuserName=ICATUSER
icatuserPW=myicatuserpasswd

# Must contain "glassfish/domains"
glassfish=/home/glassfish3/glassfish3

# Parameters for glassfish admin calls (normally 4848, admin and passwordfile)
port=4848
adminuser=admin
passwordfile=passwordfile

# The ICAT ear file
icat=/home/glassfish3/icat40/icat-ear-1583-http.ear
